{
  "name": "puz/dynamic-mail",
  "description": "Change Laravel mail driver configurations during runtime.",
  "license": "MIT",
  "authors": [
    {
      "name": "Morten Mehus",
      "email": "contact@mortenmehus.no"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "illuminate/support": "^6.0",
    "illuminate/mail": "^6.0",
    "illuminate/notifications":"^6.0"
  },
  "autoload": {
    "psr-4": {
      "Puz\\DynamicMail\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Puz\\DynamicMail\\DynamicMailServiceProvider"
      ],
      "aliases": {
        "DynamicMail": "Puz\\DynamicMail\\Facades\\DynamicMail"
      }
    }
  }
}
